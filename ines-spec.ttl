@prefix ines: <ines-spec:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<ines-spec:constraint:constant> a owl:DatatypeProperty ;
    rdfs:comment "Right hand side of the constraint." ;
    rdfs:domain ines:constraint ;
    rdfs:range rdfs:Literal .

<ines-spec:constraint:sense> a owl:DatatypeProperty ;
    rdfs:comment "Is the sense of the constraint equal to, greater than or less than." ;
    rdfs:domain ines:constraint ;
    rdfs:range rdfs:Literal .

<ines-spec:link:availability> a owl:DatatypeProperty ;
    rdfs:comment "[ratio] e.g. 0.9 means 90%. Fraction of capacity available for transfer link flows. Time series (or constant or array of periods)." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:availability_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Availability forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:capacity> a owl:DatatypeProperty ;
    rdfs:comment "[MW] Capacity of one link in either direction for one transfer link. Constant." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:constraint_link_count_coefficient> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Multiplier between constraints and the number of the links. Map of constraints and respective coefficients." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:efficiency> a owl:DatatypeProperty ;
    rdfs:comment "[ratio] Efficiency of transfer in either direction. Not used by all link_tranfer_methods. Constant, array of periods or time series." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:efficiency_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[ratio] Efficiency forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:fixed_co2_emissions> a owl:DatatypeProperty ;
    rdfs:comment "[ton of CO2/MW/year] Emissions caused by the existense of the asset. Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:fixed_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MW/year] Fixed annual cost for the transfer capacity. Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:interest_rate> a owl:DatatypeProperty ;
    rdfs:comment "[ratio] e.g. 0.05 is 5 %. Interest rate for transfer link investments. Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:investment_co2_emissions> a owl:DatatypeProperty ;
    rdfs:comment "[ton of CO2/MW] Emissions embedded in the assets to be constructed. Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:investment_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Investment cost for the transfer capacity. Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:investment_method> a owl:DatatypeProperty ;
    rdfs:comment "How investments are treated." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:investment_uses_integer> a owl:DatatypeProperty ;
    rdfs:comment "Whether investment decisions are using integer variables." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:is_non_synchronous> a owl:DatatypeProperty ;
    rdfs:comment "Chooses whether the link connects the two nodes synchronously or not." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:lifetime> a owl:DatatypeProperty ;
    rdfs:comment """[years] Lifetime of the unit for annuity calculation and for retirement decision when using retire_at_the_end_of_lifetime method. Constant or array of periods.\r
""" ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:links_existing> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of existing transfer links. Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:links_fix_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of transfer links that must exist (considers existing, invested and retired units). Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:links_invest_fix_period> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of links that must be invested in. Array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:links_invest_max_period> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of links that can be invested in. Array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:links_invest_min_period> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of links that must at least be invested in. Array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:links_max_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of transfer links that can exist (considers existing, invested and retired units). Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:links_min_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of transfer links that must at least exist (considers existing, invested and retired units). Constant or array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:links_retire_period> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of links that must be retired. Array of periods." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:operational_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Variable cost for the flow. Applies in both directions when transfer_method includes costs. Constant, array of periods or time series." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:operational_cost_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Variable operational cost forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:retirement_method> a owl:DatatypeProperty ;
    rdfs:comment "How retirements are treated." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:salvage_value> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MW] Salvage value of a transfer link at the end of lifetime (constant) or after reaching a particular age (map of periods with age and salvage value)." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link:transfer_method> a owl:DatatypeProperty ;
    rdfs:comment "How the link transfers." ;
    rdfs:domain ines:link ;
    rdfs:range rdfs:Literal .

<ines-spec:link__node__reserve:contingency_causing> a owl:DatatypeProperty ;
    rdfs:comment "Whether the flow causes a need for a contingency reserve." ;
    rdfs:domain ines:link__node__reserve ;
    rdfs:range rdfs:Literal .

<ines-spec:link__node__reserve:max_reserve_provision> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Maximum share of the flow that can be allocated to reserve provision. Constant." ;
    rdfs:domain ines:link__node__reserve ;
    rdfs:range rdfs:Literal .

<ines-spec:link__node__reserve:reservation_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh/h] Cost of providing the reservation. Constant, array of periods or time series." ;
    rdfs:domain ines:link__node__reserve ;
    rdfs:range rdfs:Literal .

<ines-spec:link__node__reserve:reserve_provision_coefficient> a owl:DatatypeProperty ;
    rdfs:comment "[ratio] e.g. 0.8 means that 80% of the provided reserve is contributing to the reserve requirement. Constant." ;
    rdfs:domain ines:link__node__reserve ;
    rdfs:range rdfs:Literal .

<ines-spec:link__node__reserve:reserve_requirement_factor> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] The factor is multiplied with the flow to increase the reserve requirement. Constant." ;
    rdfs:domain ines:link__node__reserve ;
    rdfs:range rdfs:Literal .

<ines-spec:node:co2_content> a owl:DatatypeProperty ;
    rdfs:comment "[ton of CO2/MWh] Implicit CO2 content released when fuel used. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:commodity_limit> a owl:DatatypeProperty ;
    rdfs:comment "[MWh] Maximum amount of commodity that can be extracted or deposed (negative value). Constant (cumulative limit) or a map of periods (applies separately to each period)." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:commodity_price> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh or other unit] Price of the commodity. Constant, array of periods or time series." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:commodity_price_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Commodity price forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:commodity_price_ladder> a owl:DatatypeProperty ;
    rdfs:comment "[€/MWh and MWh] Array of maps with an increasing price and a quantity at that price." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:constraint_storage_count_coefficient> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Multiplier between constraints and the number of the storage units. Map of constraints and respective coefficients." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:constraint_storage_state_coefficient> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Multiplier between constraints and the state of the storage. Map of constraints and respective coefficients." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:flow_annual> a owl:DatatypeProperty ;
    rdfs:comment "[MWh] Annual flow to scale flow_profile depending on the flow_scaling_method. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:flow_profile> a owl:DatatypeProperty ;
    rdfs:comment "[MWh or ratio] Inflow is positive and outflow (demand) is negative. Time series, constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:flow_profile_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[MWh or ratio] Inflow is positive and outflow (demand) is negative. Time series, constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:flow_scaling_method> a owl:DatatypeProperty ;
    rdfs:comment "How to scale/use the flow_profile." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:node_type> a owl:DatatypeProperty ;
    rdfs:comment "How the node should behave." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:penalty_downward> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Penalty for removing energy from the node. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:penalty_upward> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Penalty for adding energy to the node. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_capacity> a owl:DatatypeProperty ;
    rdfs:comment "[MWh] Capacity of one storage unit. Constant." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_cost_of_stored_energy> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh/h] Cost of storing energy over time. Constant or map of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_fixed_co2_emissions> a owl:DatatypeProperty ;
    rdfs:comment "[ton of CO2/MWh/year] Emissions caused by the existense of the asset. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_fixed_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh/year] Fixed annual cost for the storage capacity. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_interest_rate> a owl:DatatypeProperty ;
    rdfs:comment "[ratio] e.g. 0.05 is 5 %. Interest rate for storage investments. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_investment_co2_emissions> a owl:DatatypeProperty ;
    rdfs:comment "[ton of CO2/MWh] Emissions embedded in the assets to be constructed. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_investment_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Investment cost for the storage capacity. Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_investment_method> a owl:DatatypeProperty ;
    rdfs:comment "How investments are treated." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_investment_uses_integer> a owl:DatatypeProperty ;
    rdfs:comment "Whether investment decisions are using integer variables." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_lifetime> a owl:DatatypeProperty ;
    rdfs:comment "[years] Lifetime of the storage plant for annuity calculation and for retirement decision depending on the storage_retirement_method. Constant." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_limit_method> a owl:DatatypeProperty ;
    rdfs:comment "What limits are applied to the storage state." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_loss_absolute> a owl:DatatypeProperty ;
    rdfs:comment "[MWh/hour] Loss of stored energy over one hour. Constant or map of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_loss_from_storage_capacity> a owl:DatatypeProperty ;
    rdfs:comment "[p.u./hour] Loss of stored energy over one hour as a fraction of total storage capacity. Constant or map of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_loss_from_stored_energy> a owl:DatatypeProperty ;
    rdfs:comment "[fraction/hour] Loss of stored energy over one hour as a fraction of the energy stored. Constant or map of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_retirement_method> a owl:DatatypeProperty ;
    rdfs:comment "How retirements are treated." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_salvage_value> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Salvage value of a storage at the end of lifetime (constant) or after reaching a particular age (map of periods with age and salvage value)." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_binding_method> a owl:DatatypeProperty ;
    rdfs:comment "How storages behave when there are jumps in the timeline. 'leap_over_forward_only' just leaps over the holes connecting last hour to the first hour. 'leap_over_within_period' leaps over holes but also circles back from the end of the period to the beginning of the period. 'leap_over_within_solve' leaps over holes and also between periods but circles back from the end of the solve to the beginning. 'fill_with_representatives' fills the holes using the behaviour of the included periods using 'storage_state_representative_map'. 'fill_with_interlaced_representatives' fills the holes with linear combinations of representative periods based on 'storage_state_interlaced_mapping'." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_downward_absolute_values> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Downward price ladder for storage state using MWh (absolute) piecewise points. MWh value and price/cost in CUR/MWh." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_downward_relative_values> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Downward price ladder for storage state using per unit (relative) piecewise points. Per unit value and price/cost in CUR/MWh." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_fix> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Fix the storage state according to the storage_state_fix_method. Time series, constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_fix_method> a owl:DatatypeProperty ;
    rdfs:comment "How storage level is possibly fixed over time using 'storage_state_fix' parameter. Often used for rolling window models. 'fix_start' fixes only the fist timestep of the model. 'fix_horizon_end' fixes the end state of the storage in every solve. 'fix_start_and_horizon_end' does both. 'fix_all_timesteps' fixes the storage level to predeterminend time series or constant." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_lower_limit> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Minimum limit for the state of the storage. Time series, constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_upper_limit> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Maximum limit for the state of the storage. Time series, constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_upward_absolute_values> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Upward price ladder for storage state using MWh (absolute) piecewise points. MWh value and price/cost in CUR/MWh." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_upward_relative_values> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Upward price ladder for storage state using per unit (relative) piecewise points. Per unit value and price/cost in CUR/MWh." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_value> a owl:DatatypeProperty ;
    rdfs:comment "[CUR] Value (price) of storage state. Constant, time series or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_value_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Storage state value forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storage_state_value_method> a owl:DatatypeProperty ;
    rdfs:comment "How to include value of storage state in the objective function. end_of_solve: the last timestep of the solve will get storage_state_value. all_timesteps will apply the storage value to each timestep in the model horizon." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storages_existing> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of storages at the start of the model. Constant or map of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storages_fix_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of storage units that must exist (considers existing, invested and retired units). Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storages_invest_fix_period> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of storage units that must be invested in. Array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storages_invest_max_period> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of storage units that can be invested in. Array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storages_invest_min_period> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of storage units that must at least be invested in. Array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storages_max_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of storage units that can exist (considers existing, invested and retired units). Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storages_min_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of storage units that must at least exist (considers existing, invested and retired units). Constant or array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node:storages_retire_period> a owl:DatatypeProperty ;
    rdfs:comment "[Number] of storage units that must be retired. Array of periods." ;
    rdfs:domain ines:node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:capacity> a owl:DatatypeProperty ;
    rdfs:comment "[MW] Capacity in left-to-right direction for one transfer link. Replaces link capacity in that direction. Constant." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:constraint_flow_coefficient> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Multiplier between constraints and the left-to-right flow in the link. Should not be applied in the no_losses_no_cost transfer_method. Map of constraints and respective coefficients." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:efficiency> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Efficiency of transfer in left-to-right direction. Not used by all link_tranfer_methods. Constant, array of periods or time series." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:efficiency_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[ratio] Efficiency forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:operational_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Variable cost for the flow in left-to-right direction. Applies when transfer_method includes costs. Constant, array of periods or time series." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:operational_cost_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Variable operational cost forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:ramp_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MW] Cost of ramping up. Constant or array of periods." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:ramp_limit_down> a owl:DatatypeProperty ;
    rdfs:comment "[p.u./minute] Maximum ramp down speed. Constant." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:ramp_limit_up> a owl:DatatypeProperty ;
    rdfs:comment "[p.u./minute] Maximum ramp up speed. Constant." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__link__node:ramp_method> a owl:DatatypeProperty ;
    rdfs:comment "How ramps are constrained and/or valued." ;
    rdfs:domain ines:node__link__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__node:diffusion_coefficient> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Multiplier for diffusing energy between two nodes based on the difference in the node storage state. Constant or time series." ;
    rdfs:domain ines:node__node ;
    rdfs:range rdfs:Literal .

<ines-spec:node__node:junction_type> a owl:DatatypeProperty ;
    rdfs:comment "What is the interaction type between the two nodes." ;
