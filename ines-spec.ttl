    rdfs:range rdfs:Literal .

<ines-spec:set__reserve:gate_closure> a owl:DatatypeProperty ;
    rdfs:comment "[duration] How much time there is between the procurement of reserves (fixing the reserve contributions) and when the reserve provision starts." ;
    rdfs:domain ines:set__reserve ;
    rdfs:range rdfs:Literal .

<ines-spec:set__reserve:reserve_reactivation_time> a owl:DatatypeProperty ;
    rdfs:comment "[duration] How soon after provision of reserve the reserves must be available again." ;
    rdfs:domain ines:set__reserve ;
    rdfs:range rdfs:Literal .

<ines-spec:set__reserve:reserve_requirement> a owl:DatatypeProperty ;
    rdfs:comment "[MW] A shared reserve requirement for the set of nodes. Reserve can be provided into any of the nodes. Constant or time series." ;
    rdfs:domain ines:set__reserve ;
