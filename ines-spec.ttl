    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:profile_limit_lower_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Minimum flow limit forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:profile_limit_upper> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Maximum flow limit. Time series, constant or array of periods." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:profile_limit_upper_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[p.u.] Maximum flow limit forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:profile_method> a owl:DatatypeProperty ;
    rdfs:comment "How to limit the flow based on profiles." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:ramp_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MW] Cost of ramping up. Constant or array of periods." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:ramp_limit_down> a owl:DatatypeProperty ;
    rdfs:comment "[p.u./minute] Maximum ramp down speed. Constant." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:ramp_limit_up> a owl:DatatypeProperty ;
    rdfs:comment "[p.u./minute] Maximum ramp up speed. Constant." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:ramp_method> a owl:DatatypeProperty ;
    rdfs:comment "How ramps are constrained and/or valued." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:salvage_value> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MW] Salvage value at the end of lifetime (constant) or after reaching a particular age (map of periods with age and salvage value)." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:so2_emission_rate> a owl:DatatypeProperty ;
    rdfs:comment "[kg of SO2 emissions/MWh] SO2 emission rate per MWh of flow. Constant." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:period:duration> a owl:DatatypeProperty ;
    rdfs:comment "[Duration] of the period. Needs to use Duration parameter type." ;
    rdfs:domain ines:period ;
    rdfs:range rdfs:Literal .

<ines-spec:period:start_time> a owl:DatatypeProperty ;
    rdfs:comment "[Date_time] for the start of the period" ;
    rdfs:domain ines:period ;
    rdfs:range rdfs:Literal .

<ines-spec:period:years_represented> a owl:DatatypeProperty ;
    rdfs:comment "How many years the period represents before the next period in the solve. Used for discounting. Can be below one (multiple periods in one year)." ;
    rdfs:domain ines:period ;
    rdfs:range rdfs:Literal .

<ines-spec:reserve:reserve_type> a owl:DatatypeProperty ;
    rdfs:comment "What kind of reserve is in question (symmetric means that the reserve must be provided in both directions, contingency means that the reserve requirement is based on the contingency_causing flows)." ;
    rdfs:domain ines:reserve ;
    rdfs:range rdfs:Literal .

<ines-spec:set:capacity_margin> a owl:DatatypeProperty ;
    rdfs:comment "[MW] Capacity margin across the set of member nodes. Constant or array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:capacity_margin_method> a owl:DatatypeProperty ;
    rdfs:comment "How capacity margin is constrained. Dispatch_based: profile limited and storages are dispatched, thermal plants based on their availability." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:co2_max_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[ton of CO2] Cumulative limit for CO2 emissions from the member nodes. Constant or array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:co2_max_period> a owl:DatatypeProperty ;
    rdfs:comment "[ton of CO2] CO2 limit for each period separately. Array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:co2_price> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/ton] CO2 price for a set of nodes. Constant, array of periods or time series." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:flow_max_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[MW] Maximum aggregated average flow for the set members (unit__to_node, node__to_unit, link). The average value is multiplied by the model duration to get the cumulative limit (e.g. by 8760 if a single year is modelled). Applied for each period. Constant or map of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:flow_max_instant> a owl:DatatypeProperty ;
    rdfs:comment "[MW] Maximum instantenous flow for the aggregated flow of all set members. Constant or period." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:flow_min_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[MW] Maximum aggregated average flow for the set members (unit__to_node, node__to_unit, link). The average value is multiplied by the model duration to get the cumulative limit (e.g. by 8760 if a single year is modelled). Applied for each period. Constant or map of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:flow_min_instant> a owl:DatatypeProperty ;
    rdfs:comment "[MW] Minimum instantenous flow for the aggregated flow of all set members. Constant or period." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:inertia_limit> a owl:DatatypeProperty ;
    rdfs:comment "[MWs] Minimum for synchronous inertia in the group of nodes. Constant, period, time, or period__time." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:invest_max_period> a owl:DatatypeProperty ;
    rdfs:comment "[MW or MWh] Maximum investment per period to the aggregated capacity of set member flows (unit__to_node, node__to_unit, link) and storage capacity. Map of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:invest_max_total> a owl:DatatypeProperty ;
    rdfs:comment "[MW or MWh] Maximum investment to the aggregated capacity of set member flows (unit__to_node, node__to_unit, link) and storage capacity. Applies in all periods. Constant." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:invest_min_period> a owl:DatatypeProperty ;
    rdfs:comment "[MW or MWh] Minimum investment per period to the aggregated capacity of set member flows (unit__to_node, node__to_unit, link) and storage capacity. Map of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:invest_min_total> a owl:DatatypeProperty ;
    rdfs:comment "[MW or MWh] Minimum investment to the aggregated capacity of set member flows (unit__to_node, node__to_unit, link) and storage capacity. Applies in all periods. Constant." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:network_type> a owl:DatatypeProperty ;
    rdfs:comment "What is the network type for the nodes connected to the set." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:non_synchronous_limit> a owl:DatatypeProperty ;
    rdfs:comment "[share, e.g. 0.8 means 80%] The maximum share of non-synchronous generation in the set of nodes. Constant or period." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:nox_max_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[ton of NOx] Cumulative limit for NOx emissions from the member flows. Constant or array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:nox_max_period> a owl:DatatypeProperty ;
    rdfs:comment "[ton of NOx] NOx limit for each period separately from the member flows. Array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:nox_price> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/ton] NOx price for a set of member flows. Constant or array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:so2_max_cumulative> a owl:DatatypeProperty ;
    rdfs:comment "[ton of SO2] Cumulative limit for SO2 emissions from the member flows. Constant or array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:so2_max_period> a owl:DatatypeProperty ;
    rdfs:comment "[ton of SO2] SO2 limit for each period separately from the member flows. Array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:so2_price> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/ton] SO2 price for a set of member flows. Constant or array of periods." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:stochastic_forecast_interpolation_factors> a owl:DatatypeProperty ;
    rdfs:comment "[ratio] between 0-1. How much each consequent timestep, starting from the first timestep of the solve, takes from the realization time series (the rest comes from the forecast time series)." ;
