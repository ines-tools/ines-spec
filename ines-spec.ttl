    rdfs:range rdfs:Literal .

<ines-spec:set:stochastic_forecast_weights> a owl:DatatypeProperty ;
    rdfs:comment "[ratio between 0-1] Probability weight of each forecast. Name of forecast combined with its weight." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:stochastic_method> a owl:DatatypeProperty ;
    rdfs:comment "How to use forecast data. 'interpolate_time_series_forecasts' takes a set of forecast time series (usually high, central and low) and interpolates them towards the realization closer to first solve timestep. 'analysis_time_forecasts' has separate forecasts (1 or more) for each analysis timestep with the assumption the model rolls forward from analysis time to the next." ;
    rdfs:domain ines:set ;
    rdfs:range rdfs:Literal .

<ines-spec:set:time_resolution> a owl:DatatypeProperty ;
    rdfs:comment "Which time resolution the set will propagate to the set members (node, node__unit, link) overriding time_resolution from the solve_pattern." ;
