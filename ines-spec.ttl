<ines-spec:node__to_unit:nox_emission_rate> a owl:DatatypeProperty ;
    rdfs:comment "[kg of NOx emissions/MWh] NOx emission rate per MWh of flow. Constant." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:other_operational_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Variable cost for the flow. Constant, array of periods or time series." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:other_operational_cost_forecasts> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MWh] Variable operational cost forecasts either through forecast time series (1st dimension forecast name, 2nd dimension time series) or updating forecasts (1st dimension forecast name, 2nd dimension analysis time, 3rd time series)." ;
    rdfs:domain ines:node__to_unit ;
