    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:fixed_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MW/year] Fixed annual cost for the flow capacity. Constant or array of periods." ;
