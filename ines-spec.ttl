<ines-spec:node__to_unit:inertia_constant> a owl:DatatypeProperty ;
    rdfs:comment "[MWs/MW] Inertia constant for a synchronously connected unit to this node. Constant." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:investment_cost> a owl:DatatypeProperty ;
    rdfs:comment "[CUR/MW] Investment cost for the flow capacity. Constant or array of periods." ;
    rdfs:domain ines:node__to_unit ;
    rdfs:range rdfs:Literal .

<ines-spec:node__to_unit:is_non_synchronous> a owl:DatatypeProperty ;
    rdfs:comment "Chooses whether the unit is synchronously connected to this node." ;
    rdfs:domain ines:node__to_unit ;
