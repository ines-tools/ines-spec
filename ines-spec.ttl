            owl:cardinality 1 ;
            owl:onProperty ines:has_set ] .

ines:set__node a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty ines:has_set ],
        [ a owl:Restriction ;
            owl:cardinality 1 ;
            owl:onProperty ines:has_node ] .

